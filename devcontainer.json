{
    "name": "Python 3.12 Jupyter Notebook Container",
    "dockerFile": "../Dockerfile",
    "context": "..",
    "appPort": [
      8888
    ],
    "remoteUser": "root",
    "settings": {
      "python.pythonPath": "/usr/local/bin/python"
    },
    "extensions": [
      "ms-python.python",
      "ms-toolsai.jupyter"
    ],
    "forwardPorts": [
      8888
    ],
    "mounts": [
      "source=${localWorkspaceFolder}/Fizza,target=/Fizza,type=bind"
    ]
  }
  